{
  "name": "wattanar/etto",
  "description": "Etto's php micro framework base on slim 3",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "https://github.com/wattanar/etto",
  "license": "MIT",
  "authors": [
    {
      "name": "Wattana Ruengmucha",
      "email": "iamwattanas@gmail.com",
      "homepage": "https://wattana.me"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "3.10",
    "league/plates": "^3.3.0",
    "slim/csrf": "^0.8.2",
    "firebase/php-jwt": "^5.0.0",
    "wattanar/sqlsrv": "3.2.0",
    "zeuxisoo/slim-whoops": "0.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0",
    "maximebf/debugbar": "1.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "config": {
    "process-timeout": 0
  }
}
